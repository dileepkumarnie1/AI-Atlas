<script defer>
// Helper function to get the first 20 characters of the slug and replace slashes with dashes
function getSlug() {
    let url = window.location.pathname;
    
    // If the pathname is empty or just a slash, use the href instead
    if (url === '/' || url === '') {
         return 'homepage';
    }
    
    // Remove leading slash if present
    url = url.replace(/^\//, '-');
    
    // Replace slashes with dashes
    let slug = url.replace(/\//g, '-');
    
    // Take the first 25 characters
    slug = slug.substring(0, 25);
    
    console.log('Generated slug:', slug);
    return slug;
}

// Function to store the first visited URL slug in localStorage
function storeInitialSlug() {
    if (!localStorage.getItem('initialSlug')) {
        const initialSlug = getSlug();
        localStorage.setItem('initialSlug', initialSlug);
        console.log('Stored initial slug:', initialSlug);
    }
}

// Function to get the initial slug (stored) or fall back to the current one
function getInitialSlug() {
    const storedSlug = localStorage.getItem('initialSlug');
    const currentSlug = getSlug();
    
    console.log('Stored slug:', storedSlug);
    console.log('Current slug:', currentSlug);
    
    const slug = storedSlug || currentSlug;
    console.log('Using slug:', slug);
    return slug;
}

// Function to update UTM parameters for specific links while preserving existing mktcmpid and utm_campaign
function updateUTMParameters() {
    // Get all links on the page
    const links = document.querySelectorAll('a');
    
    // Get the current page's slug for UTM parameters
    const currentSlug = getSlug();
    console.log('Current slug for UTM parameters:', currentSlug);

    links.forEach(link => {
        const href = link.href;
        const url = new URL(href);

        // Handle Chrome Web Store links
        if (href.startsWith('https://chromewebstore.google.com/')) {
            url.searchParams.set('utm_source', 'website');
            url.searchParams.set('utm_medium', 'organic');

            // Only set utm_campaign if it doesn't already exist
            if (!url.searchParams.has('utm_campaign')) {
                url.searchParams.set('utm_campaign', currentSlug);
            }

            link.href = url.toString();
        }

        // Handle AppSource (Microsoft) links
        if (href.startsWith('https://appsource.microsoft.com/')) {
            // Only set mktcmpid if it doesn't already exist
            if (!url.searchParams.has('mktcmpid')) {
                url.searchParams.set('mktcmpid', currentSlug);
            }
            
            // Always set src=website
            url.searchParams.set('src', 'website');

            link.href = url.toString();
        }
    });
}

// Store the initial slug when the page loads for the first time
document.addEventListener('DOMContentLoaded', () => {
    console.log('DOM Content Loaded');
    storeInitialSlug();
    updateUTMParameters();
});

// Cookie Consent Banner
const cookieBanner = document.getElementById('cookie-banner');
const acceptCookies = document.getElementById('accept-cookies');
const dismissBanner = document.getElementById('dismiss-banner');

// Function to set a cookie
function setCookie(name, value, days) {
    const date = new Date();
    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
    const expires = "expires=" + date.toUTCString();
    document.cookie = name + "=" + value + ";" + expires + ";path=/";
}

// Function to get a cookie
function getCookie(name) {
    const nameEQ = name + "=";
    const cookiesArray = document.cookie.split(';');
    for (let i = 0; i < cookiesArray.length; i++) {
        let cookie = cookiesArray[i].trim();
        if (cookie.indexOf(nameEQ) == 0) {
            return cookie.substring(nameEQ.length, cookie.length);
        }
    }
    return null;
}

// Display the banner if consent cookie is not set
window.onload = function() {
    if (!getCookie("cookieConsent")) {
        cookieBanner.style.display = "block";
    }
};

// Set consent cookie and hide the banner
acceptCookies.addEventListener('click', function() {
    setCookie("cookieConsent", "true", 365); // 1-year expiration
    cookieBanner.style.display = "none";
});

// Dismiss the banner without setting the cookie
dismissBanner.addEventListener('click', function() {
    cookieBanner.style.display = "none";
});

// Add hover effects for the button
acceptCookies.addEventListener("mouseover", function () {
    this.style.backgroundColor = "#000";
    this.style.color = "#fff";
});

acceptCookies.addEventListener("mouseout", function () {
    this.style.backgroundColor = "white";
    this.style.color = "#7468ff";
});
</script>
