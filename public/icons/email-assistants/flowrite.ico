/* Pulse effect on the pseudo-elements */
#write-button::before,
#write-button::after {
  content: '';
  position: absolute;
  top: 50%;
  left: 40%;
  width: 18px; /* size of the circle */
  height: 18px; /* size of the circle */
  background-color: transparent;
  border: 2px solid #fff; /* Blue border to match button color */
  border-radius: 50%;
  transform: translate(-50%, -50%);
  animation: pulse 2s infinite ease-in-out;
}

#write-button:hover::before,
#write-button:hover::after {
  border: 2px solid var(--background-color--background-primary);
}
#copy-button::before,
#copy-button::after{
	content: '';
  position: absolute;
  top: 50%;
  left: 70%;
  width: 18px; /* size of the circle */
  height: 18px; /* size of the circle */
  background-color: transparent;
  border: 2px solid var(--background-color--background-primary);; /* Blue border to match button color */
  border-radius: 50%;
  transform: translate(-50%, -50%);
  animation: pulse 2s infinite ease-in-out;
}
#copy-button::after {
  animation-delay: 1s; /* Staggered pulsing effect */
}

#write-button::after {
  animation-delay: 1s; /* Staggered pulsing effect */
}

/* Pulsing animation for the circles */
@keyframes pulse {
  0% {
    transform: translate(-50%, -50%) scale(0);
    opacity: 1;
  }
  50% {
    transform: translate(-50%, -50%) scale(1.5);
    opacity: 0.5;
  }
  100% {
